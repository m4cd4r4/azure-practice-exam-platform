# Development Environment Configuration
# Azure Practice Exam Platform

# API Configuration
REACT_APP_API_BASE_URL=https://azpracticeexam-dev-functions.azurewebsites.net/api
REACT_APP_ENVIRONMENT=development

# Debug Settings
REACT_APP_DEBUG=true
REACT_APP_ENABLE_OFFLINE=false
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_BETA=true

# Network Configuration
REACT_APP_RETRY_ATTEMPTS=3
REACT_APP_REQUEST_TIMEOUT=10000

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_PROGRESS_SAVING=true