# Production Environment Configuration
# Azure Practice Exam Platform

# API Configuration
REACT_APP_API_BASE_URL=https://azpracticeexam-prod-functions.azurewebsites.net/api
REACT_APP_ENVIRONMENT=production

# Debug Settings
REACT_APP_DEBUG=false
REACT_APP_ENABLE_OFFLINE=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_BETA=false

# Network Configuration
REACT_APP_RETRY_ATTEMPTS=5
REACT_APP_REQUEST_TIMEOUT=15000

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_PROGRESS_SAVING=true